<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@calendar.css" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DBShowController">
    <children>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="423.0" prefWidth="556.0">
            <columns>
                <TableColumn prefWidth="75.0" text="ID">
                <cellValueFactory>
                    <PropertyValueFactory property="id" />
                </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="179.0" text="Date">
                    <cellValueFactory>
                        <PropertyValueFactory property="date" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="301.0" text="Events">
                    <cellValueFactory>
                        <PropertyValueFactory property="events" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <ScrollBar layoutX="577.0" layoutY="48.0" orientation="VERTICAL" prefHeight="387.0" prefWidth="18.0" />
        <Button layoutX="357.0" layoutY="446.0" mnemonicParsing="false" onAction="#deleteData" prefHeight="31.0" prefWidth="100.0" text="delete" />
        <TextField fx:id="deleteID" layoutX="158.0" layoutY="446.0" promptText="Insert delete id" />
        <Button layoutX="470.0" layoutY="446.0" mnemonicParsing="false" onAction="#back" prefHeight="31.0" prefWidth="100.0" text="Back" />
    </children>
</AnchorPane>
